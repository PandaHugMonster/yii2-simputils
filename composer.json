{
  "name": "spaf/yii2-simputils",
  "version": "0.1.1",
  "description": "Yii2 extension wrap for php-simputils, some useful wraps that improve usage",
  "license": "MIT",
  "authors": [
    {
      "name": "Ivan Ponomarev",
      "email": "pi@spaf.dev"
    }
  ],
  "type": "yii2-extension",
  "minimum-stability": "stable",
  "require": {
    "php": ">=8.0",
    "spaf/simputils": "~0.3 || ~1",
    "yiisoft/yii2": "~2.0.43"
  },
  "autoload": {
    "psr-4": {
      "spaf\\yii\\simputils\\": "src/"
    },
    "files": [
      "src/basic.php"
    ]
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
