{
  "name": "spaf/yii2-simputils",
  "version": "0.2.0",
  "description": "Yii2 extension wrap for SimpUtils, some useful wraps that improve usage",
  "license": "MIT",
  "authors": [
    {
      "name": "Ivan Ponomarev",
      "email": "pi@spaf.dev"
    }
  ],
  "type": "yii2-extension",
  "minimum-stability": "stable",
  "require": {
    "php": ">=8.0",
    "spaf/simputils": "~0.3 || ~1",
    "yiisoft/yii2": "~2.0.42"
  },
  "autoload": {
    "psr-4": {
      "spaf\\simputils\\yii\\": "src/"
    },
    "files": [
      "src/basic.php"
    ]
  },
  "repositories": [
    {
      "package": [{
        "name": "spaf/simputils"
      }
      ],
      "type": "path",
      "url": "/home/ivan/development/php-simputils"
    },
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
